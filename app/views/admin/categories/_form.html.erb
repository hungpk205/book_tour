<%= form_for [:admin, @category] do |f|%>
  <%= render "shared/error_messages", object: f.object %>

  <%= f.label :name, t(".name") %>
  <%= f.text_field :name, class: "form-control" %>

  <%= f.collection_select :parent_id, @parent_cats, :id, :name, include_blank: true, class: "form-control" %>

  <%= f.submit yield(:btn_create), class: "btn btn-primary" %>
<%end%>
